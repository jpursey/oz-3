## Variants:
##    SHL.W <reg>, <reg>
##    SHL.W <reg>, <1..16>
##    SHL.D <dreg>, <reg>
##    SHL.D <dreg>, <1..32>
##
## Logical shift left of the register by the specified number of bits. The last
## bit shifted out sets the value of the "C" flag.
################################################################################

macro(bits:5) LogicalShiftLeft16(p) {
  code "$r" {
    # Shift zero bits?
    TST(m);
    JC(Z,@zero);

    # Shift > 16 bits?
    CMPI(m,17);
    JC(NS,@max);

    # Shift 1-16 bits
    MOV(C2,m);
  @shift:
    SL(p);
    JD(C2,@shift);
    MSR(ZSCO,ZSCO);
    END;

  @max:
    MOVI(p,0);
    MSC(SCO); MSS(Z); MSR(SCO,Z);
    END;

  @zero:
    TST(p); MSR(ZSCO,ZSCO);
  }

  code "1" {
    SL(p);
    MSR(ZSCO,ZSCO);
  }
  code "2" {
    SL(p); SL(p);
    MSR(ZSCO,ZSCO);
  }
  code "3" {
    SL(p); SL(p); SL(p);
    MSR(ZSCO,ZSCO);
  }
  code "4" {
    SL(p); SL(p); SL(p); SL(p);
    MSR(ZSCO,ZSCO);
  }
  code "5" {
    SL(p); SL(p); SL(p); SL(p); SL(p);
    MSR(ZSCO,ZSCO);
  }
  code "6" {

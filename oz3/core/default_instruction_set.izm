################################################################################
## RST <core-index> <bank-mask>
##
## Cycles: ...
## ZSCO: ----
##
## The core to reset may be specified as an immediate value 0-7 or the special
## keyword SELF (to reset the running core), or specified by register R2 (0..7,
## or -1 to indiciate self).
##
## The core's memory banks are specified in R3 in the same configuration as in
## the MB register, depending on the bank mask:
##   Bit 0: CODE
##     The core's BC register is set to this cores R4 register.
##     The core's IP register is set to zero.
##     The core's R0 and R1 registers are copied from this core.
##   Bit 1: STACK
##     The core's BS register is set to this cores R5 register.
##     The core's FP and SP registers are set to zero.
##   Bit 2: DATA
##     The core's BD register is set to this cores R6 register.
##   Bit 3: EXTRA
##     The core's BE register is set to this cores Ry register.
################################################################################

macro LockCore {
  code "$#3" { CLK(i); }
  code "R2" { CLK(R2); }
  code "SELF" { MOVI(C2,-1); CLK(C2); }
}

instruction RST "$m, $#4" {
  UL;
  $LockCore;

@code:
  # Test if code is set
  MSKI(C1,1);
  JC(Z, @stack);

  # Update code registers
  MVN(C0:0,R3:0);
  CBK(CODE,C0);
  CST(BC,R4);
  MOVI(C0,0);
  CST(IP,C0);
  CST(R0,R0);
  CST(R1,R1);

@stack:
  # Test if stack is set
  MSKI(C1,2);
  JC(Z, @data);

  # Update stack registers
  MVN(C0:0,R3:1);
  CBK(STACK,C0);
  CST(BS,R5);
  MOVI(C0,0);
  CST(SP,C0);
  CST(FP,C0);

@data:
  SR(MB); SR(MB); SR(MB); SR(MB);

  # Test if data is set
  MSKI(C1,4);
  JC(Z, @data);

  # Update data registers
  MVN(C0:0,R3:2);
  CBK(DATA,C0);
  CST(BD,R6);

@extra:
  # Test if extra is set
  MSKI(C1,8);
  JC(Z, @end);

  # Update extra registers
  MVN(C0:0,R3:3);
  CBK(EXTRA,C0);
  CST(BE,R7);

@end:
  CUL;
}

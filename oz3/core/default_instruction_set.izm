    MSR(ZSCO,ZSCO);
  }
  code "7" {
    RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p);
    MSR(ZSCO,ZSCO);
  }
  code "8" {
    RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p);
    MSR(ZSCO,ZSCO);
  }
  code "9" {
    RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p);
    RL(p);
    MSR(ZSCO,ZSCO);
  }
  code "10" {
    RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p);
    RL(p); RL(p);
    MSR(ZSCO,ZSCO);
  }
  code "11" {
    RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p); RL(p);
    RL(p); RL(p); RL(p);
    MSR(ZSCO,ZSCO);
  }
